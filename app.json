{
  "expo": {
    "name": "My bakery",
    "slug": "my-bakery",
    "version": "1.0.0",
    "orientation": "portrait",
    "platforms": ["ios", "android"],
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundlerIdentifier": "bakery",
      "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "android": {
      "package": "bakery",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "CAMERA",
        "ACCESS_FINE_LOCATION",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "config": {
      "googleMaps": {
        "apiKey": "AIzaSyBxw05K0kcBBzxUWOGpwCwuejLRqFt2B-k"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
